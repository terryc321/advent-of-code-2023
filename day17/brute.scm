;; 1 1 based = pad *vec* out so do not need to subtract on each xy 

;; for now we stick with 0 0 based approach 
(define (xy x y)
  (vector-ref (vector-ref *vec* y) x))

(define (vec-height v)
  (vector-length v))

(define (vec-width v)
  (vector-length (vector-ref v 0)))

(define *height* (vec-height *vec*))
(define *width* (vec-width *vec*))

(define (onboard? x y)
  (and (>= x 0) (< x *width*)
       (>= y 0) (< y *height*)))

(define (next-state x y score m1 m2 m3)  
  (cond
   ((eq? m1 'left) (next-state-left x y score m1 m2 m3))
   ((eq? m1 'right) (next-state-right x y score m1 m2 m3))
   ((eq? m1 'up) (next-state-up x y score m1 m2 m3))
   ((eq? m1 'down) (next-state-down x y score m1 m2 m3))
   (#t (error "next-state bad direction"))))

(define (record-state x y score m1 m2 m3)
  (format #t "(~a ~a ~a ~a ~a ~a)~%" x y score m1 m2 m3))

;; ----- decouple next-state from record-state -------

(define-syntax macro-left
  (er-macro-transformer
   (lambda (expr rename compare?)
    `(let* ((x2 (- x 1))(y2 y))
       (when (onboard? x2 y2)
	 (let* ((score2 (+ score (xy x2 y2))))
	   (record-state x2 y2 score2 'left m1 m2)))))))
   
(define-syntax macro-right
  (er-macro-transformer
   (lambda (expr rename compare?)
    `(let* ((x2 (+ x 1))(y2 y))
       (when (onboard? x2 y2)
	 (let* ((score2 (+ score (xy x2 y2))))
	   (record-state x2 y2 score2 'right m1 m2)))))))

(define-syntax macro-up
  (er-macro-transformer
   (lambda (expr rename compare?)
    `(let* ((x2 x)(y2 (- y 1)))
       (when (onboard? x2 y2)
	 (let* ((score2 (+ score (xy x2 y2))))
	   (record-state x2 y2 score2 'up m1 m2)))))))

(define-syntax macro-down
  (er-macro-transformer
   (lambda (expr rename compare?)
    `(let* ((x2 x)(y2 (+ y 1)))
       (when (onboard? x2 y2)
	 (let* ((score2 (+ score (xy x2 y2))))
	   (record-state x2 y2 score2 'down m1 m2)))))))


;; ----- state transitions ---------
(define (next-state-right x y score m1 m2 m3)
  (cond
   ((and (eq? m1 'right) (eq? m2 'right) (eq? m3 'right)) #f)
   (#t (macro-right)))
  (macro-down)
  (macro-up))

(define (next-state-left x y score m1 m2 m3)
  (cond
   ((and (eq? m1 'left) (eq? m2 'left) (eq? m3 'left)) #f)
   (#t (macro-left)))
  (macro-down)
  (macro-up))

(define (next-state-up x y score m1 m2 m3)
  (cond
   ((and (eq? m1 'up) (eq? m2 'up) (eq? m3 'up)) #f)
   (#t (macro-up)))
  (macro-left)
  (macro-right))

(define (next-state-down x y score m1 m2 m3)
  (cond
   ((and (eq? m1 'down) (eq? m2 'down) (eq? m3 'down)) #f)
   (#t (macro-down)))
  (macro-left)
  (macro-right))


;; ----- loop -----
(define (solve)
  (next-state 1 0 (xy 1 0) 'right #f #f)
  (next-state 0 1 (xy 0 1) 'down #f #f))



;; ------- file io ---------
(define (recover)
  (call-with-output-file "rescue"
    (lambda (port)
      (format port "~a~%" *score*)
      (flush-output port))))



















